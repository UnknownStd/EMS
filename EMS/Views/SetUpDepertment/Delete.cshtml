@model EMS.Models.SetUpDepertmentViewModel
@{
	ViewData["Title"] = "Delete Department";
}
 <h1>Departmetn Details</h1>
<br /> 
<table class="p-3">
	<tbody>
	<tr>
		<td class="px-3 fw-bold">Department Name:</td>
		<td>@Model.DepartmentName</td>
	</tr>	
	<tr>
			<td class="px-3 fw-bold">Display No:</td>
		<td>@Model.DisplayNo</td>
	</tr>	
	</tbody>
</table>
<form class="border-success" asp-controller="SetUpDepertment" asp-action="Delete">

	<input asp-for="DepartmentId" style="display:none;" />
	<input type="submit" value="Delete" class="btn btn-dark my-3" />

</form>	